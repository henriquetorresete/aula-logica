var idade = prompt("qual sua idade");

if (idade >= 16) {
  console.log("apto a votar");
}else if (idade <16) {
  console.log("nao pode votar");
}

CODIGO =  DE QUEM É O BEM?

var item = prompt("enforme seu bem");


if (item == "casa"){
  console.log ("esse bem é de caio")
}
if (item == "bike"){
  console.log ("é de henrique")
}
if (item == "tv") {
  console.log("isso é da cely")
  }
  
